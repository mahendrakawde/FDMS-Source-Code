BODY {
	margin: 0px; 
}
H6 {
	font-family : Arial, Helvetica, sans-serif;
	color : #000080;
	font-size : medium;
	font-weight : bold;
}
H4 {
	position: relative;
	text-indent : 67%;
	page-break-before : avoid;
}
SPAN.subhead {
	font-size : smaller;
}
TD.heading{
	text-align : right;
} 
TD.data {
	text-align : left;
}

DIV.menu1Options {
	POSITION: absolute; display: none;
}
DIV.menu2Options {
	POSITION: absolute; display: none;
}
A.dropMenu:link {
	POSITION: relative; WIDTH: 105; BORDER-BOTTOM: #040600 1px solid; BORDER-LEFT: #040600  1px solid; BORDER-RIGHT: #040600  1px solid; DISPLAY: block; PADDING-TOP: 4px; PADDING-BOTTOM: 4px; PADDING-RIGHT: 0px; PADDING-LEFT: 5px; FONT-SIZE: 11px; FONT-FAMILY: arial, times, verdana; COLOR: #366696; FONT-WEIGHT: bold; TEXT-DECORATION: none; TEXT-ALIGN: left;
}
A.dropMenu:visited {
	POSITION: relative; WIDTH: 105; BORDER-BOTTOM: #040600 1px solid; BORDER-LEFT: #040600  1px solid; BORDER-RIGHT: #040600  1px solid; DISPLAY: block; PADDING-TOP: 4px; PADDING-BOTTOM: 4px; PADDING-RIGHT: 0px; PADDING-LEFT: 5px; FONT-SIZE: 11px; FONT-FAMILY: arial, times, verdana; COLOR: #366696; FONT-WEIGHT: bold; TEXT-DECORATION: none; TEXT-ALIGN: left;
}
A.dropMenu:hover {
	POSITION: relative; WIDTH: 105; BORDER-BOTTOM: #040600 1px solid; BORDER-LEFT: #040600  1px solid; BORDER-RIGHT: #040600  1px solid; DISPLAY: block; PADDING-TOP: 4px; PADDING-BOTTOM: 4px; PADDING-RIGHT: 0px; PADDING-LEFT: 5px; FONT-SIZE: 11px; FONT-FAMILY: arial, times, verdana; COLOR: #8B04B2; FONT-WEIGHT: bold; TEXT-DECORATION: none; TEXT-ALIGN: left;
}
A.menu {
	POSITION: relative; display: block; 
}
A .menuButton{
	border : none;
}
A.text {
	FONT-SIZE: 14px; COLOR: #000080; LINE-HEIGHT: normal; FONT-FAMILY: arial, times, verdana; TEXT-DECORATION: none
}
A.textBig {
	FONT-SIZE: 24px; COLOR: #000080; LINE-HEIGHT: normal; FONT-FAMILY: arial, times, verdana; TEXT-DECORATION: none
}
A.textBold {
	FONT-SIZE: 14px; COLOR: #000080; LINE-HEIGHT: normal; FONT-FAMILY: arial, times, verdana; FONT-WEIGHT: bold; TEXT-DECORATION: none
}
A.textBigBold {
	FONT-SIZE: 24px; COLOR: #000080; LINE-HEIGHT: normal; FONT-FAMILY: arial, times, verdana; FONT-WEIGHT: bold; TEXT-DECORATION: none
}
A.messageTitle {
	FONT-SIZE: 14px;
	COLOR: #000080;
	LINE-HEIGHT: normal;
	FONT-FAMILY: arial, times, verdana;
	FONT-WEIGHT: bold;
	TEXT-DECORATION: underline;
	TEXT-ALIGN: center;
	font-style: normal;

}
A.messageText {
	FONT-SIZE: 12px; COLOR: #000080; LINE-HEIGHT: normal; FONT-FAMILY: arial, times, verdana; TEXT-DECORATION: none
}
A.featuresText {
	FONT-SIZE: 12px; COLOR: #000080; LINE-HEIGHT: normal; FONT-FAMILY: arial, times, verdana; TEXT-DECORATION: none
}
a.messageBlink {
	FONT-SIZE: 14px;
	COLOR: #FF0000;
	LINE-HEIGHT: normal;
	FONT-FAMILY: arial, times, verdana;
	TEXT-DECORATION: underline;
	font-weight: bold;

}
a.black:link {
	color: #000000;
	text-decoration: underline;
}
a.black:visited {
	color: #000000;
	text-decoration: underline;
}
a.black:hover {
	color: red;
	text-decoration: underline;
}
.bannerheading {
	font-family: Arial, Helvetica, sans-serif;
	font-size: medium;
	font-style: normal;
	font-weight: bold;
	background-color: #CCFFFF;
	border: solid;
	text-align: center;

}
.noborder {
	border: none;
}
noborder {
	border: none;
}
.tips {
	font-family: Arial, Helvetica, sans-serif;
	font-size: xx-small;
	font-style: italic;
	border: thin dotted #3333FF;
}
.headingLarge {
	font-family: Arial, Helvetica, sans-serif;
	font-size: medium;
	font-style: italic;
	font-weight: bold;
	color: #000080;
	text-align: right;
}
input.numberRight {
	text-align: right;
}
input.textCenter {
	text-align: center;
}
.additionalHelpText {

	font-family: Arial, Helvetica, sans-serif;
	font-size: xx-small;
	font-style: italic;
	border: thin none #3333FF;
}
.RequiredAbbit {
	font-family: Arial, Helvetica, sans-serif;
	font-size: larger;
	font-style: normal;
	font-weight: bold;
	color: #0000CC;
}
.borderCell {

	border: thin inset #33FFFF;
}
