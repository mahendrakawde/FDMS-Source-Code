<%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html" %>
<%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic" %>
<logic:greaterThan name="User" property="securityInventory" value="0">
<td align="left">
   <html:link target="openNewWindow('Inventory','');" styleClass="menu" forward="showInventory"><html:img alt="Inventory" src="images-old/inventory.jpg" border="0" /></html:link> 
</td>
</logic:greaterThan>
