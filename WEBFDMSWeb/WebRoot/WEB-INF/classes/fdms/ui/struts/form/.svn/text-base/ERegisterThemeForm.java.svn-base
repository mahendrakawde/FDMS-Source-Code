package fdms.ui.struts.form;

import java.util.List;

import org.apache.struts.action.ActionForm;
import org.apache.struts.upload.FormFile;

import com.aldorassist.webfdms.model.ERegisterBackgroundThemeDTO;
import com.aldorassist.webfdms.model.ERegisterThemeDTO;
import com.aldorassist.webfdms.model.PresentationImageDTO;
import com.aldorassist.webfdms.model.ServiceScreenThemeDTO;

public class ERegisterThemeForm extends ActionForm {

	private static final long serialVersionUID = 1L;

	private boolean defaultPlaceHolderAvailable;
	private long themeId;
	private String themeDesc;
	private List<ERegisterThemeDTO> themes;
	private PresentationImageDTO defaultPlaceHolder;
	private List<ERegisterBackgroundThemeDTO> backgroundThemes;
	private List<ServiceScreenThemeDTO> serviceScreenThemes;
	private String backgroundThemeDesc;
	private String serviceScreenThemeDesc;
	private String operation;
	
	private FormFile file;

	public boolean isDefaultPlaceHolderAvailable() {
		return defaultPlaceHolderAvailable;
	}
	public void setDefaultPlaceHolderAvailable(boolean defaultPlaceHolderAvailable) {
		this.defaultPlaceHolderAvailable = defaultPlaceHolderAvailable;
	}

	public long getThemeId() {
		return themeId;
	}
	public void setThemeId(long themeId) {
		this.themeId = themeId;
	}

	public String getThemeDesc() {
		return themeDesc;
	}
	public void setThemeDesc(String themeDesc) {
		this.themeDesc = themeDesc;
	}
	
	public List<ERegisterThemeDTO> getThemes() {
		return themes;
	}
	public void setThemes(List<ERegisterThemeDTO> themes) {
		this.themes = themes;
	}
	
	public PresentationImageDTO getDefaultPlaceHolder() {
		return defaultPlaceHolder;
	}
	public void setDefaultPlaceHolder(PresentationImageDTO defaultPlaceHolder) {
		this.defaultPlaceHolder = defaultPlaceHolder;
	}

	public FormFile getFile() {
		return file;
	}
	public void setFile(FormFile file) {
		this.file = file;
	}
	
	public List<ERegisterBackgroundThemeDTO> getBackgroundThemes() {
		return backgroundThemes;
	}
	public void setBackgroundThemes(
			List<ERegisterBackgroundThemeDTO> backgroundThemes) {
		this.backgroundThemes = backgroundThemes;
	}

	public List<ServiceScreenThemeDTO> getServiceScreenThemes() {
		return serviceScreenThemes;
	}
	public void setServiceScreenThemes(
			List<ServiceScreenThemeDTO> serviceScreenThemes) {
		this.serviceScreenThemes = serviceScreenThemes;
	}
	
	public String getBackgroundThemeDesc() {
		return backgroundThemeDesc;
	}
	public void setBackgroundThemeDesc(String backgroundThemeDesc) {
		this.backgroundThemeDesc = backgroundThemeDesc;
	}

	public String getServiceScreenThemeDesc() {
		return serviceScreenThemeDesc;
	}
	public void setServiceScreenThemeDesc(String serviceScreenThemeDesc) {
		this.serviceScreenThemeDesc = serviceScreenThemeDesc;
	}

	public String getOperation() {
		return operation;
	}
	public void setOperation(String operation) {
		this.operation = operation;
	}

}
