package fdms.ui.struts.form;

import org.apache.struts.action.ActionForm;

public class FinanceChargeSelectBase extends ActionForm {
  
  /**
	 * 
	 */
	private static final long serialVersionUID = 1725086226779512640L;
private int x ;  
  private String directive ;  
  private int y ;
  
  public String getDirective () {
	return directive ;
  }  
  public int getX () {
	return x ;
  }  
  public int getY () {
	return y ;
  }  
  public void setDirective (String in) {
	directive = in;
  }  
  public void setX (int in) {
	x = in;
  }  
  public void setY (int in) {
	y = in;
  }  
}
