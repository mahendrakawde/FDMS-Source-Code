package fdms.ui.struts.form;

import org.apache.struts.action.ActionForm;

public class FinancialChangePriceListFormBase extends ActionForm {

	/**
	 * 
	 */
	private static final long serialVersionUID = 7628236397942861602L;

	private String priceChange;

	private String insertPriceChangeAtCurrentPosition;

	private String addPriceChangeAtEnd;

	public String getAddPriceChangeAtEnd() {
		return addPriceChangeAtEnd;
	}

	public String getInsertPriceChangeAtCurrentPosition() {
		return insertPriceChangeAtCurrentPosition;
	}

	public String getPriceChange() {
		return priceChange;
	}

	public void setAddPriceChangeAtEnd(String in) {
		addPriceChangeAtEnd = in;
	}

	public void setInsertPriceChangeAtCurrentPosition(String in) {
		insertPriceChangeAtCurrentPosition = in;
	}

	public void setPriceChange(String in) {
		priceChange = in;
	}
}
