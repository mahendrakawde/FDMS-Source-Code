package fdms.ui.struts.form;

import org.apache.struts.action.ActionForm;

public class DonationsFormBase extends ActionForm {
  
  /**
	 * 
	 */
	private static final long serialVersionUID = -6475079662847328177L;
private String donation ;
  private String deceasedFullName;
  private String submitbutton;
  
	 public String getDeceasedFullName () {
	return deceasedFullName ;
  }  
  public String getDonation () {
	return donation ;
  }  
   public String getSubmitbutton () {
	return submitbutton ;
  }  
  public void setDeceasedFullName (String in) {
	deceasedFullName = in;
  }  
  public void setDonation (String in) {
	donation = in;
  }  
  public void setSubmitbutton (String in) {
	submitbutton = in;
  }  
}
