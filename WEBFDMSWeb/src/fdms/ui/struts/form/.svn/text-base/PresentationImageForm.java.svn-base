package fdms.ui.struts.form;

import java.util.List;

import org.apache.struts.action.ActionForm;
import org.apache.struts.upload.FormFile;

import com.aldorassist.webfdms.model.PresentationImageDTO;

public class PresentationImageForm extends ActionForm {

	private static final long serialVersionUID = 1L;

	private long imageId;
	private String imageName;
	private String imagePath;
	private long themeId;
	private String operation;
	private FormFile file;
	private List<PresentationImageDTO> presentationImages;

	public long getImageId() {
		return imageId;
	}
	public void setImageId(long imageId) {
		this.imageId = imageId;
	}

	public String getImageName() {
		return imageName;
	}
	public void setImageName(String imageName) {
		this.imageName = imageName;
	}
	
	public String getImagePath() {
		return imagePath;
	}
	public void setImagePath(String imagePath) {
		this.imagePath = imagePath;
	}

	public long getThemeId() {
		return themeId;
	}
	public void setThemeId(long themeId) {
		this.themeId = themeId;
	}

	public String getOperation() {
		return operation;
	}
	public void setOperation(String operation) {
		this.operation = operation;
	}

	public FormFile getFile() {
		return file;
	}
	public void setFile(FormFile file) {
		this.file = file;
	}

	public List<PresentationImageDTO> getPresentationImages() {
		return presentationImages;
	}
	public void setPresentationImages(List<PresentationImageDTO> presentationImages) {
		this.presentationImages = presentationImages;
	}
	
}
