	public void setVisitationStatus(int index, String in) {
		logger.debug ("Visitation Status for index " + index + ": " + in);
		DbVisitations visitation = getVisitation(index);
		char temp = in.charAt(0);
		visitation.setStatus(temp);
		logger.debug ("Visitation Status after write: " + visitation.getStatus());
		visitations.set(index, visitation);
	}
	
	public void setVisitationId(int index, int in) {
		DbVisitations visitation = getVisitation(index);
		visitation.setId(in);
		visitations.set(index, visitation);	
	}
	
	private DbVisitations getVisitation(int index) {
		DbVisitations visitation;
		try {
			visitation = (DbVisitations) visitations.get(index);
		}
		catch (IndexOutOfBoundsException e) {
			while (visitations.size() <= index)
				visitations.add(new DbVisitations());
			visitation = (DbVisitations) visitations.get(index);
		}
		return visitation;
	}

