package fdms.messenger;

import java.util.List;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlElementWrapper;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement(name="ServiceInfo")
public class ServiceInfo {

	private String heldAt;
	private String serviceCity;
	private String serviceDate;
	private String serviceHour;
	private String Clergy1;
	private String Clergy2;
	private String Clergy3;
	private String SermonNotes;
	private List<String> Bearers;
	private List<String> musicSelections;
	private List<String> specialSelections;
	private String ceremonyBy;
	private String dispositionPlace;
	private String disposition;
	private String DispositionCity;
	private String DispositionCounty;
	private String DispositionState;
	private String section;
	private String block;
	private String lot;
	private String buriedHour;
	private String buriedDay;
	private String buriedMonth;
	private String buriedYear;

	

	public ServiceInfo() {
		super();
		this.heldAt = null;
		this.serviceCity = null;
		this.serviceDate = null;
		this.serviceHour = null;
		Clergy1 = null;
		Clergy2 = null;
		Clergy3 = null;
		SermonNotes = null;
		Bearers = null;
		this.musicSelections = null;
		this.specialSelections = null;
		this.ceremonyBy = null;
		this.dispositionPlace = null;
		this.disposition = null;
		DispositionCity = null;
		DispositionCounty = null;
		DispositionState = null;
		this.section = null;
		this.block = null;
		this.lot = null;
		this.buriedHour = null;
		this.buriedDay = null;
		this.buriedMonth = null;
		this.buriedYear = null;
	}

	
	public ServiceInfo(String heldAt, String serviceCity, String serviceDate,
			String serviceHour, String clergy1, String clergy2, String clergy3,
			String sermonNotes, List<String> bearers, List<String> musicSelections,
			List<String> specialSelections, String ceremonyBy,
			String dispositionPlace, String disposition, String dispositionCity,
			String dispositionCounty, String dispositionState, String section,
			String block, String lot, String buriedHour, String buriedDay,
			String buriedMonth, String buriedYear) {
		super();
		this.heldAt = heldAt;
		this.serviceCity = serviceCity;
		this.serviceDate = serviceDate;
		this.serviceHour = serviceHour;
		Clergy1 = clergy1;
		Clergy2 = clergy2;
		Clergy3 = clergy3;
		SermonNotes = sermonNotes;
		Bearers = bearers;
		this.musicSelections = musicSelections;
		this.specialSelections = specialSelections;
		this.ceremonyBy = ceremonyBy;
		this.dispositionPlace = dispositionPlace;
		this.disposition = disposition;
		DispositionCity = dispositionCity;
		DispositionCounty = dispositionCounty;
		DispositionState = dispositionState;
		this.section = section;
		this.block = block;
		this.lot = lot;
		this.buriedHour = buriedHour;
		this.buriedDay = buriedDay;
		this.buriedMonth = buriedMonth;
		this.buriedYear = buriedYear;
	}

	@XmlElement(name = "HeldAt")
	public String getHeldAt() {
		return heldAt;
	}

	public void setHeldAt(String heldAt) {
		this.heldAt = heldAt;
	}

	@XmlElement(name = "ServiceCity")
	public String getServiceCity() {
		return serviceCity;
	}

	public void setServiceCity(String serviceCity) {
		this.serviceCity = serviceCity;
	}

	@XmlElement(name = "ServiceDate")
	public String getServiceDate() {
		return serviceDate;
	}

	public void setServiceDate(String serviceDate) {
		this.serviceDate = serviceDate;
	}

	@XmlElement(name = "ServiceHour")
	public String getServiceHour() {
		return serviceHour;
	}

	public void setServiceHour(String serviceHour) {
		this.serviceHour = serviceHour;
	}

	@XmlElement(name = "Clergy1")
	public String getClergy1() {
		return Clergy1;
	}

	public void setClergy1(String clergy1) {
		Clergy1 = clergy1;
	}

	@XmlElement(name = "Clergy2")
	public String getClergy2() {
		return Clergy2;
	}

	public void setClergy2(String clergy2) {
		Clergy2 = clergy2;
	}

	@XmlElement(name = "Clergy3")
	public String getClergy3() {
		return Clergy3;
	}

	public void setClergy3(String clergy3) {
		Clergy3 = clergy3;
	}

	@XmlElement(name = "SermonNotes")
	public String getSermonNotes() {
		return SermonNotes;
	}

	public void setSermonNotes(String sermonNotes) {
		SermonNotes = sermonNotes;
	}

	@XmlElementWrapper(name = "Bearers")
	@XmlElement(name = "string")
	public List<String> getBearers() {
		return Bearers;
	}

	public void setBearers(List<String> bearers) {
		Bearers = bearers;
	}

	@XmlElementWrapper(name = "MusicSelections")
	@XmlElement(name = "string")
	public List<String> getMusicSelections() {
		return musicSelections;
	}

	public void setMusicSelections(List<String> musicSelections) {
		this.musicSelections = musicSelections;
	}

	@XmlElementWrapper(name = "SpecialSelections")
	@XmlElement(name = "string")
	public List<String> getSpecialSelections() {
		return specialSelections;
	}

	public void setSpecialSelections(List<String> specialSelections) {
		this.specialSelections = specialSelections;
	}

	@XmlElement(name = "CeremonyBy")
	public String getCeremonyBy() {
		return ceremonyBy;
	}

	public void setCeremonyBy(String ceremonyBy) {
		this.ceremonyBy = ceremonyBy;
	}

	@XmlElement(name = "DispositionPlace")
	public String getDispositionPlace() {
		return dispositionPlace;
	}

	public void setDispositionPlace(String dispositionPlace) {
		this.dispositionPlace = dispositionPlace;
	}

	@XmlElement(name = "Disposition")
	public String getDisposition() {
		return disposition;
	}

	public void setDisposition(String disposition) {
		this.disposition = disposition;
	}

	@XmlElement(name = "DispositionCity")
	public String getDispositionCity() {
		return DispositionCity;
	}

	public void setDispositionCity(String dispositionCity) {
		DispositionCity = dispositionCity;
	}

	@XmlElement(name = "DispositionCounty")
	public String getDispositionCounty() {
		return DispositionCounty;
	}

	public void setDispositionCounty(String dispositionCounty) {
		DispositionCounty = dispositionCounty;
	}

	@XmlElement(name = "DispositionState")
	public String getDispositionState() {
		return DispositionState;
	}

	public void setDispositionState(String dispositionState) {
		DispositionState = dispositionState;
	}

	@XmlElement(name = "Section")
	public String getSection() {
		return section;
	}

	public void setSection(String section) {
		this.section = section;
	}

	@XmlElement(name = "Block")
	public String getBlock() {
		return block;
	}

	public void setBlock(String block) {
		this.block = block;
	}

	@XmlElement(name = "Lot")
	public String getLot() {
		return lot;
	}

	public void setLot(String lot) {
		this.lot = lot;
	}

	@XmlElement(name = "BuriedHour")
	public String getBuriedHour() {
		return buriedHour;
	}

	public void setBuriedHour(String buriedHour) {
		this.buriedHour = buriedHour;
	}

	@XmlElement(name = "BuriedDay")
	public String getBuriedDay() {
		return buriedDay;
	}

	public void setBuriedDay(String buriedDay) {
		this.buriedDay = buriedDay;
	}

	@XmlElement(name = "BuriedMonth")
	public String getBuriedMonth() {
		return buriedMonth;
	}

	public void setBuriedMonth(String buriedMonth) {
		this.buriedMonth = buriedMonth;
	}

	@XmlElement(name = "BuriedYear")
	public String getBuriedYear() {
		return buriedYear;
	}

	public void setBuriedYear(String buriedYear) {
		this.buriedYear = buriedYear;
	}
