package com.aldorsolutions.webfdms.checkwriter.model;

/**
 * 
 * @author David Rollins 
 * Date: Jun 25, 2007 
 * File: ApVendorDTO.java
 * 
 */
public class ApVendorLocationDTO {
	
	private long vendorLocationID;
	
	private long vendorID;
	
	private long locationID;
	
	private long localeID;
	
	private long companyID;
	
	private int defaultAcctID;
	

	/**
	 * @return the localeID
	 */
	public long getLocaleID() {
		return localeID;
	}

	/**
	 * @return the locationID
	 */
	public long getLocationID() {
		return locationID;
	}

	/**
	 * @return the vendorID
	 */
	public long getVendorID() {
		return vendorID;
	}

	/**
	 * @return the vendorLocationID
	 */
	public long getVendorLocationID() {
		return vendorLocationID;
	}


	/**
	 * @param localeID
	 *            the localeID to set
	 */
	public void setLocaleID(long localeID) {
		this.localeID = localeID;
	}

	/**
	 * @param locationID
	 *            the locationID to set
	 */
	public void setLocationID(long locationID) {
		this.locationID = locationID;
	}

	/**
	 * @param vendorID
	 *            the vendorID to set
	 */
	public void setVendorID(long vendorID) {
		this.vendorID = vendorID;
	}

	/**
	 * @param vendorLocationID
	 *            the vendorLocationID to set
	 */
	public void setVendorLocationID(long vendorLocationID) {
		this.vendorLocationID = vendorLocationID;
	}
	
	public long getCompanyID() {
		return companyID;
	}

	public void setCompanyID(long companyID) {
		this.companyID = companyID;
	}

	public String toString () {
		
		StringBuilder sb = new StringBuilder();
		
		sb.append( "vendorLocationID = " + vendorLocationID + "; " );
		sb.append( "vendorID = " + vendorID + "; " );
		sb.append( "locationID = " + locationID + "; " );
		sb.append( "localeID = " + localeID + "; " );
		sb.append( "defaultAcctID = " + defaultAcctID + "; " );
		return ( sb.toString() );
	}

	/**
	 * @return the defaultAcctID
	 */
	public int getDefaultAcctID() {
		return defaultAcctID;
	}

	/**
	 * @param defaultAcctID the defaultAcctID to set
	 */
	public void setDefaultAcctID(int defaultAcctID) {
		this.defaultAcctID = defaultAcctID;
	}
}