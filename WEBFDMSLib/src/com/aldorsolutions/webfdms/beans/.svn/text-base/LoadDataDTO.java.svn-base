package com.aldorsolutions.webfdms.beans;


public class LoadDataDTO {
	
	public static final String LOW_PRIORITY = "LOW_PRIORITY";
	public static final String CONCURRENT = "CONCURRENT";
	public static final String LOCAL = "LOCAL";
	public static final String REPLACE = "REPLACE";
	public static final String IGNORE = "IGNORE";
		
	
	private String dataBaseName;
	private String tableName;
	private String priority;
	private boolean isLocal;
	private String fileName;
	private boolean replace;
	private String filedTerminator;
	private String fieldEnclouser;
	private String escapeChar;
	private String lineStartingby;
	private String lineTerminatedBy;
	private String[] colList;
	
	public LoadDataDTO(){
		this.priority = LOW_PRIORITY;
		this.isLocal = true;
		this.replace = true;
		this.filedTerminator = ",";
		this.escapeChar = "\\\\";
		this.fieldEnclouser="\"";
		this.lineTerminatedBy = "\\r\\n";
	}
	
	/**
	 * @return the dataBaseName
	 */
	public String getDataBaseName() {
		return dataBaseName;
	}
	/**
	 * @param dataBaseName the dataBaseName to set
	 */
	public void setDataBaseName(String dataBaseName) {
		this.dataBaseName = dataBaseName;
	}
	/**
	 * @return the escapeChar
	 */
	public String getEscapeChar() {
		return escapeChar;
	}
	/**
	 * @param escapeChar the escapeChar to set
	 */
	public void setEscapeChar(String escapeChar) {
		this.escapeChar = escapeChar;
	}
	/**
	 * @return the fieldEnclouser
	 */
	public String getFieldEnclouser() {
		return fieldEnclouser;
	}
	/**
	 * @param fieldEnclouser the fieldEnclouser to set
	 */
	public void setFieldEnclouser(String fieldEnclouser) {
		this.fieldEnclouser = fieldEnclouser;
	}
	/**
	 * @return the filedTerminator
	 */
	public String getFiledTerminator() {
		return filedTerminator;
	}
	/**
	 * @param filedTerminator the filedTerminator to set
	 */
	public void setFiledTerminator(String filedTerminator) {
		this.filedTerminator = filedTerminator;
	}
	
	/**
	 * @return the isLocal
	 */
	public boolean isLocal() {
		return isLocal;
	}
	/**
	 * @param isLocal the isLocal to set
	 */
	public void setLocal(boolean isLocal) {
		this.isLocal = isLocal;
	}
	/**
	 * @return the isReplace
	 */
	public boolean isReplace() {
		return replace;
	}
	/**
	 * @param isReplace the isReplace to set
	 */
	public void setReplace(boolean isReplace) {
		this.replace = isReplace;
	}
	/**
	 * @return the lineStartingby
	 */
	public String getLineStartingby() {
		return lineStartingby;
	}
	/**
	 * @param lineStartingby the lineStartingby to set
	 */
	public void setLineStartingby(String lineStartingby) {
		this.lineStartingby = lineStartingby;
	}
	/**
	 * @return the lineTerminatedBy
	 */
	public String getLineTerminatedBy() {
		return lineTerminatedBy;
	}
	/**
	 * @param lineTerminatedBy the lineTerminatedBy to set
	 */
	public void setLineTerminatedBy(String lineTerminatedBy) {
		this.lineTerminatedBy = lineTerminatedBy;
	}
	/**
	 * @return the priority
	 */
	public String getPriority() {
		return priority;
	}
	/**
	 * @param priority the priority to set
	 */
	public void setPriority(String priority) {
		this.priority = priority;
	}
	/**
	 * @return the tableName
	 */
	public String getTableName() {
		return tableName;
	}
	/**
	 * @param tableName the tableName to set
	 */
	public void setTableName(String tableName) {
		this.tableName = tableName;
	}
	/**
	 * @return the colList
	 */
	public String[] getColList() {
		return colList;
	}
	/**
	 * @param colList the colList to set
	 */
	public void setColList(String[] colList) {
		this.colList = colList;
	}

	/**
	 * @return the fileName
	 */
	public String getFileName() {
		return fileName;
	}

	/**
	 * @param fileName the fileName to set
	 */
	public void setFileName(String fileName) {
		this.fileName = fileName;
	}

	

}

