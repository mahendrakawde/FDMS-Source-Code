package com.aldorsolutions.webfdms.reporting.crystal.model;

public class FormAvailableFilterTypeDTO {

	public static final int FILTER_DATE = 1;
	public static final int FILTER_LOCATION = 2;
	public static final int FILTER_LOCALE = 3;
	
	private int formFilterTypeID;

	private String filterName;

	private String description;

	private int defaultParameter;

	private int defaultValue;

	/**
	 * @return the defaultParameter
	 */
	public int getDefaultParameter() {
		return defaultParameter;
	}

	/**
	 * @return the defaultValue
	 */
	public int getDefaultValue() {
		return defaultValue;
	}

	/**
	 * @return the description
	 */
	public String getDescription() {
		return description;
	}

	/**
	 * @return the filterName
	 */
	public String getFilterName() {
		return filterName;
	}

	/**
	 * @return the formFilterID
	 */
	public int getFormFilterTypeID() {
		return formFilterTypeID;
	}

	/**
	 * @param defaultParameter
	 *            the defaultParameter to set
	 */
	public void setDefaultParameter(int defaultParameter) {
		this.defaultParameter = defaultParameter;
	}

	/**
	 * @param defaultValue
	 *            the defaultValue to set
	 */
	public void setDefaultValue(int defaultValue) {
		this.defaultValue = defaultValue;
	}

	/**
	 * @param description
	 *            the description to set
	 */
	public void setDescription(String description) {
		this.description = description;
	}

	/**
	 * @param filterName
	 *            the filterName to set
	 */
	public void setFilterName(String filterName) {
		this.filterName = filterName;
	}

	/**
	 * @param formFilterID
	 *            the formFilterID to set
	 */
	public void setFormFilterTypeID(int formFilterTypeID) {
		this.formFilterTypeID = formFilterTypeID;
	}

}
