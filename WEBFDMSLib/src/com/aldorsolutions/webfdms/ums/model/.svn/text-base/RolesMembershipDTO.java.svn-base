package com.aldorsolutions.webfdms.ums.model;

public class RolesMembershipDTO {

	private long userID = 0;

	private long roleID = 0;

	/**
	 * 
	 */
	public RolesMembershipDTO() {
		super();
	}

	/**
	 * @param userID
	 * @param roleID
	 */
	public RolesMembershipDTO(long userID, long roleID) {
		super();
		this.userID = userID;
		this.roleID = roleID;
	}

	/**
	 * @return the roleID
	 */
	public long getRoleID() {
		return roleID;
	}

	/**
	 * @return the userID
	 */
	public long getUserID() {
		return userID;
	}

	/**
	 * @param roleID
	 *            the roleID to set
	 */
	public void setRoleID(long roleID) {
		this.roleID = roleID;
	}

	/**
	 * @param userID
	 *            the userID to set
	 */
	public void setUserID(long userID) {
		this.userID = userID;
	}
	
}