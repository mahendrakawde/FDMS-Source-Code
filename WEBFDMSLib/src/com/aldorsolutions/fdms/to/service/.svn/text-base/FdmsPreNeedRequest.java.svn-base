package com.aldorsolutions.fdms.to.service;

import com.aldorsolutions.fdms.to.common.*;
import com.aldorsolutions.fdms.to.fdmscase.*;

public class FdmsPreNeedRequest extends FdmsCaseRequest {
	
	private RelatedPerson buyer = RelatedPerson.getDefault();
	private String coBuyer = "";
	private Person insured ;
	private SalesInfo salesInfo;
	private FundDepository fundDepository;
	private String source = "";
	private String notes = "";
	
	public FdmsPreNeedRequest() {}

	public RelatedPerson getBuyer() {
		return buyer;
	}

	public void setBuyer(RelatedPerson buyer) {
		this.buyer = buyer;
	}

	public String getCoBuyer() {
		return coBuyer;
	}

	public void setCoBuyer(String coBuyer) {
		this.coBuyer = coBuyer;
	}

	public Person getInsured() {
		return insured;
	}

	public void setInsured(Person insured) {
		this.insured = insured;
	}

	public SalesInfo getSalesInfo() {
		return salesInfo;
	}

	public void setSalesInfo(SalesInfo saleInfo) {
		this.salesInfo = saleInfo;
	}

	public FundDepository getFundDepository() {
		return fundDepository;
	}

	public void setFundDepository(FundDepository fundDepository) {
		this.fundDepository = fundDepository;
	}

	public String getSource() {
		return source;
	}

	public void setSource(String source) {
		this.source = source;
	}

	public String getNotes() {
		return notes;
	}

	public void setNotes(String notes) {
		this.notes = notes;
	}
	
}
