package com.aldorsolutions.fdms.to.common;

import java.util.ArrayList;
import java.util.List;

import com.aldorsolutions.fdms.to.vitals.VitalsIdInfo;

public class VitalsIdInfos {

	private List<VitalsIdInfo> infos = new ArrayList<VitalsIdInfo>();

	public VitalsIdInfos() {}
	
	public void addInfos(VitalsIdInfo vitalsIdInfo) {
		if (vitalsIdInfo != null) {
			infos.add(vitalsIdInfo);
		}
	}
	
	public List<VitalsIdInfo> getInfos() {
		return infos;
	}

	public void setInfos(List<VitalsIdInfo> infos) {
		this.infos = infos;
	}
	
}
