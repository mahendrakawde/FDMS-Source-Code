package com.aldorassist.webfdms.model;

import java.sql.Timestamp;

public class ComboDataDTO {
	
		/**
		 * Use COMBODATA_LOCAL_DEFULT to indicate the default options
		 */
	static public final int COMBODATA_LOCAL_DEFULT = 0;
	
	static public final int COMBODATA_TYPE_ACCOUNT_DESCRIPTION = 1;
	static public final int COMBODATA_TYPE_SALES_TYPE = 2;
	static public final int COMBODATA_TYPE_MISC_CASH_DESCRIPTION = 3;
	
	private long comboDataID;
	private long local;
	private int type;
	private String name;
	private String value;
	private String description;
	private Timestamp createdDTS;
	private Timestamp updatedDTS;
	
	
	
	/**
	 * @param local
	 * @param type
	 * @param name
	 * @param value
	 * @param description
	 * @param createdDTS
	 * @param updatedDTS
	 */
	public ComboDataDTO(long local, int type, String name, String value, String description, Timestamp createdDTS,
			Timestamp updatedDTS) {
		this.local = local;
		this.type = type;
		this.name = name;
		this.value = value;
		this.description = description;
		this.createdDTS = createdDTS;
		this.updatedDTS = updatedDTS;
	}



	public ComboDataDTO() {
		// TODO Auto-generated constructor stub
		this.local = 0;
		this.type = 0;
		this.name = null;
		this.value = null;
		this.description = null;
		this.createdDTS = null;
		this.updatedDTS = null;
	}



	/**
	 * @return the comboDataID
	 */
	public long getComboDataID() {
		return comboDataID;
	}



	/**
	 * @param comboDataID the comboDataID to set
	 */
	public void setComboDataID(long comboDataID) {
		this.comboDataID = comboDataID;
	}



	/**
	 * @return the local
	 */
	public long getLocal() {
		return local;
	}



	/**
	 * @param local the local to set
	 */
	public void setLocal(long local) {
		this.local = local;
	}



	/**
	 * @return the type
	 */
	public int getType() {
		return type;
	}



	/**
	 * @param type the type to set
	 */
	public void setType(int type) {
		this.type = type;
	}



	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}



	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}



	/**
	 * @return the value
	 */
	public String getValue() {
		return value;
	}



	/**
	 * @param value the value to set
	 */
	public void setValue(String value) {
		this.value = value;
	}



	/**
	 * @return the description
	 */
	public String getDescription() {
		return description;
	}



	/**
	 * @param description the description to set
	 */
	public void setDescription(String description) {
		this.description = description;
	}



	/**
	 * @return the createdDTS
	 */
	public Timestamp getCreatedDTS() {
		return createdDTS;
	}



	/**
	 * @param createdDTS the createdDTS to set
	 */
	public void setCreatedDTS(Timestamp createdDTS) {
		this.createdDTS = createdDTS;
	}



	/**
	 * @return the updatedDTS
	 */
	public Timestamp getUpdatedDTS() {
		return updatedDTS;
	}



	/**
	 * @param updatedDTS the updatedDTS to set
	 */
	public void setUpdatedDTS(Timestamp updatedDTS) {
		this.updatedDTS = updatedDTS;
	}

}
