public class ApVendorLocationDisplayDTO {
	
	private long vendorLocationID;
	
	private String locationName;
	
	private String localeName;
	
	private String defaultAcct;
	
	private String defaultAcctDesc;
	
	/**
	 * @return the defaultAcctDesc
	 */
	public String getDefaultAcctDesc() {
		return defaultAcctDesc;
	}

	/**
	 * @param defaultAcctDesc the defaultAcctDesc to set
	 */
	public void setDefaultAcctDesc(String defaultAcctDesc) {
		this.defaultAcctDesc = defaultAcctDesc;
	}

	/**
	 * @return the defaultAcct
	 */
	public String getDefaultAcct() {
		return defaultAcct;
	}

	/**
	 * @return the vendorLocationID
	 */
	public long getVendorLocationID() {
		return vendorLocationID;
	}

	/**
	 * @param defaultAcct
	 *            the defaultAcct to set
	 */
	public void setDefaultAcct(String defaultAcct) {
		this.defaultAcct = defaultAcct;
	}

	/**
	 * @param vendorLocationID
	 *            the vendorLocationID to set
	 */
	public void setVendorLocationID(long vendorLocationID) {
		this.vendorLocationID = vendorLocationID;
	}

	/**
	 * @return the localeName
	 */
	public String getLocaleName() {
		return localeName;
	}

	/**
	 * @return the locationName
	 */
	public String getLocationName() {
		return locationName;
	}

	/**
	 * @param localeName the localeName to set
	 */
	public void setLocaleName(String localeName) {
		this.localeName = localeName;
	}

	/**
	 * @param locationName the locationName to set
	 */
	public void setLocationName(String locationName) {
		this.locationName = locationName;
	}
	
}