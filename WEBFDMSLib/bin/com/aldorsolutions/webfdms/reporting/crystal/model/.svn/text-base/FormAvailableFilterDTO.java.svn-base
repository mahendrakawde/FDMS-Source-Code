package com.aldorsolutions.webfdms.reporting.crystal.model;


public class FormAvailableFilterDTO {

	private long formFilterID;

	private long formID;

	private long filterTypeID;

	private long filterParameter;

	private String filterValue;

	/**
	 * @return the filterParameter
	 */
	public long getFilterParameter() {
		return filterParameter;
	}

	/**
	 * @return the filterType
	 */
	public long getFilterTypeID() {
		return filterTypeID;
	}

	/**
	 * @return the filterValue
	 */
	public String getFilterValue() {
		return filterValue;
	}

	/**
	 * @return the formFilterID
	 */
	public long getFormFilterID() {
		return formFilterID;
	}

	/**
	 * @return the formID
	 */
	public long getFormID() {
		return formID;
	}

	/**
	 * @param filterParameter
	 *            the filterParameter to set
	 */
	public void setFilterParameter(long filterParameter) {
		this.filterParameter = filterParameter;
	}

	/**
	 * @param filterType
	 *            the filterType to set
	 */
	public void setFilterTypeID(long filterTypeID) {
		this.filterTypeID = filterTypeID;
	}

	/**
	 * @param filterValue
	 *            the filterValue to set
	 */
	public void setFilterValue(String filterValue) {
		this.filterValue = filterValue;
	}

	/**
	 * @param formFilterID
	 *            the formFilterID to set
	 */
	public void setFormFilterID(long formFilterID) {
		this.formFilterID = formFilterID;
	}

	/**
	 * @param formID
	 *            the formID to set
	 */
	public void setFormID(long formID) {
		this.formID = formID;
	}

}
