		this.name = name;
	}

	/**
	 * @param priceListID
	 *            the priceListID to set
	 */
	public void setPriceListID(long priceListID) {
		this.priceListID = priceListID;
	}

	/**
	 * @param timestamp
	 *            the timestamp to set
	 */
	public void setTimestamp(Timestamp timestamp) {
		this.timestamp = timestamp;
	}

