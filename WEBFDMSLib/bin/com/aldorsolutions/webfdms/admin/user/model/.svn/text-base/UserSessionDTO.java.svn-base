package com.aldorsolutions.webfdms.admin.user.model;

/**
 * 
 * @author Guadalupe Garcia
 */

import java.io.Serializable;

public class UserSessionDTO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -3802308242012958418L;

	private String username;

	private String database;

	private String loggedIn;

	private String lastAccessed;

	/** Creates a new instance of UserSessionVO */
	public UserSessionDTO() {
	}

	public UserSessionDTO(String username, String database, String loggedIn, String lastAccessed) {

		this.username = username;
		this.database = database;
		this.loggedIn = loggedIn;
		this.lastAccessed = lastAccessed;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public void setDatabase(String database) {
		this.database = database;
	}

	public void setLoggedIn(String loggedIn) {
		this.loggedIn = loggedIn;
	}

	public void setLastAccessed(String lastAccessed) {
		this.lastAccessed = lastAccessed;
	}

	public String getUsername() {
		return username;
	}

	public String getDatabase() {
		return database;
	}

	public String getLoggedIn() {
		return loggedIn;
	}

	public String getLastAccessed() {
		return lastAccessed;
	}

}
