package com.aldorsolutions.webfdms.admin.user.model;

import java.io.Serializable;
import java.sql.Date;

/**
 * 
 * @author David Rollins
 * Date: May 21, 2007
 * File: UserVacationDTO.java	
 *
 */
public class UserVacationDTO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -3477138541272729725L;

	private long userVacationID;
	
	private long userID;
	
	private long notifyUserID;
	
	private Date dateFrom;
	
	private Date dateTo;

	/**
	 * @return the notifyUserID
	 */
	public long getNotifyUserID() {
		return notifyUserID;
	}

	/**
	 * @return the userID
	 */
	public long getUserID() {
		return userID;
	}

	/**
	 * @return the userVacationID
	 */
	public long getUserVacationID() {
		return userVacationID;
	}

	/**
	 * @param notifyUserID the notifyUserID to set
	 */
	public void setNotifyUserID(long notifyUserID) {
		this.notifyUserID = notifyUserID;
	}

	/**
	 * @param userID the userID to set
	 */
	public void setUserID(long userID) {
		this.userID = userID;
	}

	/**
	 * @param userVacationID the userVacationID to set
	 */
	public void setUserVacationID(long userVacationID) {
		this.userVacationID = userVacationID;
	}

	/**
	 * @return the dateFrom
	 */
	public Date getDateFrom() {
		return dateFrom;
	}

	/**
	 * @return the dateTo
	 */
	public Date getDateTo() {
		return dateTo;
	}

	/**
	 * @param dateFrom the dateFrom to set
	 */
	public void setDateFrom(Date dateFrom) {
		this.dateFrom = dateFrom;
	}

	/**
	 * @param dateTo the dateTo to set
	 */
	public void setDateTo(Date dateTo) {
		this.dateTo = dateTo;
	}

}