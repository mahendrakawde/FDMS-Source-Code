package com.aldorsolutions.fdms.to.financial;


public class Charge {
	private int contractLineNo; 
	private String description = "";
	private float amount; 
	private String taxCode = "";
	private int sequenceNo ; 
	
	private InventoryItem marchandiseItem;
	private Integer serviceItemId ;
	
	private boolean fromPackage ;
	
	public Charge(){}

	public int getContractLineNo() {
		return contractLineNo;
	}

	public void setContractLineNo(int contractLineNo) {
		this.contractLineNo = contractLineNo;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public float getAmount() {
		return amount;
	}

	public void setAmount(float amount) {
		this.amount = amount;
	}

	public String getTaxCode() {
		return taxCode;
	}

	public void setTaxCode(String taxCode) {
		this.taxCode = taxCode;
	}

	public int getSequenceNo() {
		return sequenceNo;
	}

	public void setSequenceNo(int sequenceNo) {
		this.sequenceNo = sequenceNo;
	}

	public InventoryItem getMarchandiseItem() {
		return marchandiseItem;
	}

	public void setMarchandiseItem(InventoryItem marchandiseItem) {
		this.marchandiseItem = marchandiseItem;
	}

	public Integer getServiceItemId() {
		return serviceItemId;
	}

	public void setServiceItemId(Integer serviceItemId) {
		this.serviceItemId = serviceItemId;
	}

	public boolean isFromPackage() {
		return fromPackage;
	}

	public void setFromPackage(boolean fromPackage) {
		this.fromPackage = fromPackage;
	}
}
