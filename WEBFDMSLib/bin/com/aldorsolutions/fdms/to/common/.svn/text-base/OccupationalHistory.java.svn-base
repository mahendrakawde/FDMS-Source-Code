package com.aldorsolutions.fdms.to.common;

import java.util.Date;

public class OccupationalHistory {
	
	private String wasDecedentEverInUSArmedForces; 
	private Date armedForcesEntryDate;
	private Date armedForcesDepartureDate;
	private String usualOccupation = "";
	private String kindOfBusinessOrIndustry = "";
	private String currentEmployer = "";
	
	public OccupationalHistory(){}

	public String wasDecedentEverInUSArmedForces() {
		return wasDecedentEverInUSArmedForces;
	}

	public void setWasDecedentEverInUSArmedForces(
			String wasDecedentEverInUSArmedForces) {
		this.wasDecedentEverInUSArmedForces = wasDecedentEverInUSArmedForces;
	}

	public Date getArmedForcesEntryDate() {
		return armedForcesEntryDate;
	}

	public void setArmedForcesEntryDate(Date armedForcesEntryDate) {
		this.armedForcesEntryDate = armedForcesEntryDate;
	}

	public Date getArmedForcesDepartureDate() {
		return armedForcesDepartureDate;
	}

	public void setArmedForcesDepartureDate(Date armedForcesDepartureDate) {
		this.armedForcesDepartureDate = armedForcesDepartureDate;
	}

	public String getUsualOccupation() {
		return usualOccupation;
	}

	public void setUsualOccupation(String usualOccupation) {
		this.usualOccupation = usualOccupation;
	}

	public String getKindOfBusinessOrIndustry() {
		return kindOfBusinessOrIndustry;
	}

	public void setKindOfBusinessOrIndustry(String kindOfBusinessOrIndustry) {
		this.kindOfBusinessOrIndustry = kindOfBusinessOrIndustry;
	}

	public String getCurrentEmployer() {
		return currentEmployer;
	}

	public void setCurrentEmployer(String currentEmployer) {
		this.currentEmployer = currentEmployer;
	}
}
