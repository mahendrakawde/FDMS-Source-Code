html {
	padding: 0px;
	margin: 0px;
}

body {
	padding: 0px;
	margin: 0px;
	background: #d8d8d8; /* #e5e5e5; */
}

a{
text-decoration: none;
}

#tabnav p {
	padding: 0px;
	margin: 0px auto;
}

div#maintop {
	padding: 0px;
	margin: 0px 10px 0px 10px;
	float: left;
	width: 98%; /*it was 1000px*/
	height: 17px;
	background: #fff url(../images/maintopBg.jpg) repeat-x;
}

div#maintop img.left {
	float: left;
}

div#maintop img.right {
	float: right;
	
}

div#mainbottom {
	padding: 0px;
	margin: 0px;
	float: left;
	height: 17px;
	width: 100%; /*it was 1000px*/
	background: #fff url(../images/mainbottomBg.jpg) repeat-x bottom;
}

div#mainbottom img.left {
	float: left;
	
}

div#mainbottom img.right {
	float: right;
	
}

div#main {
	position: relative;
	float: left;
	padding: 0px;
	width: 98%; /*it was 1000px*/
	margin: 0px 0px 10px 10px;
	background: #fff url(../images/mainBg.jpg) repeat-y;
	font: normal 10px/10px Verdana;
	color: #000;
	z-index:-20;
}

div#main div#content {
	/*padding: 0px 10px;*/ /*it was padding: 0px 10px;*/
	margin: 1px 0px 10px 10px
	float: left;
	width: 100%; /*it was 980px*/
	background: url(../images/contentrightBg.jpg) repeat-y right;
	
}

div#content div#text {
	padding: 0px 10px; /*it was 0px*/
	margin: 0px;
	width: 98%; /*it was 880px*/
}


div.invoiceList {
	padding: 0px 0px 25px 0px;
	margin: 0px;
	width: 975px; /*it was 875*/
	overflow: auto;
	overflow-y: hidden;
}


div#user{
	height: 1px;
	
}
.userMenu
{
	color: #334455;
    font-family: Arial,sans-serif;
    font-size: 14px;
    text-decoration: none;
    font-weight: bolder;
}

