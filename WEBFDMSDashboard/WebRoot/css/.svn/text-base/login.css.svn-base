      }

   ul#nav {
	padding: 0px;
	margin: 10px 0px 0px 0px;
	width: 225px;
	height: 338px;
	list-style: none;
	float: left;
	position: relative;
	background-image: url(../images1.1/nav_bg.jpg);
	background-repeat: no-repeat;
   }
      ul#nav li {
         padding: 0px;
         margin: 0px;
         display: block;
         width: 220px;
         height: 73px;
         position: absolute;
         left: 3px;
      }
      ul#nav li#software {
         top: 4px;
      }
         li#software a:link, li#software a:visited, li#software a:active {
	display: block;
	width: 220px;
	height: 73px;
	text-decoration: none;
	background-image: url(../images1.1/nav_software_bg.jpg);
	background-repeat: no-repeat;
         }
         li#software a:hover {
            background: none;
            text-decoration: none;
         }
      ul#nav li#website {
         top: 90px;
      }
         li#website a:link, li#website a:visited, li#website a:active {
	display: block;
