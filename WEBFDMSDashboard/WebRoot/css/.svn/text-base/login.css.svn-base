  div#content {
    background: url("../images1.1/content_bg.jpg") repeat-y scroll 0 0 transparent;
    float: left;
    margin: 0;
    padding: 0 20px;
    position: relative;
    width: 522px;
}
    p#top {
    background: url("../images1.1/content_top.jpg") no-repeat scroll 0 0 transparent;
    display: block;
    height: 30px;
    margin: 0 -20px;
    padding: 0;
    width: 562px;
}
      p#bottom {
         padding: 0px;
         margin: 15px -20px 0px -20px;
         display: block;
         width: 562px;
         height: 30px;
         background: url(../images1.1/content_bottom.jpg) no-repeat;
		 
      }
      
/*                           
         LOGIN FORM          
                           */
      div#content form#login {
	padding: 0px 10px 0px 0px;
