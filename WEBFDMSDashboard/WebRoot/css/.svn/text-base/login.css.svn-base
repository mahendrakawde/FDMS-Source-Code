html {
   padding: 0px;
   margin: 0px;
}

body {
	 background: none repeat scroll 0 0 #FFFFFF;
    color: #000000;
    font: 11px/20px Verdana;
    margin: 0;
    padding: 20px;
}

/*                           
        MISCELLANEOUS        
                           */
.hide {
   display: none;
}

br.clear {
   padding: 0px;
   margin: 0px;
   width: 0px;
   height: 0px;
   clear: both;
}

.centertext {
   text-align: center;
}

a:link, a:visited, a:active {
   color: #134d78;
   text-decoration: none;
}
a:hover {
   color: #2596e9;
   text-decoration: underline;
}
div#lable{
	margin-left:170px;
	width:100px;
	color: #134d78;
	text-align:left;
	}


p#bluetext {
	color: #134d78;
	text-align:center;
	}
div#input {
	color: #134d78;
	text-align:center;
	margin-top:3px;
   }
p#version {
   text-align: center;
}
