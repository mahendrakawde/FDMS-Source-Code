div#container {
   margin: 0 auto !important;
    padding: 0;
    width: 800px;

}

   h1#aldor {
	padding: 0px;
	margin: 0px;
	display: block;
	width: 320px;
	height: 105px;
	background-image: url(../images1.1/FDMS_logo_v3s.jpg);
	/* background-image: url(../images1.1/logo_aldor.jpg); */
	background-repeat: no-repeat;
   }
      h1#aldor a:link, h1#aldor a:visited, h1#aldor a:active, h1#aldor a:hover {
