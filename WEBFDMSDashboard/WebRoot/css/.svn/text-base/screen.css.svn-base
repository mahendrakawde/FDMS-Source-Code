div#checkTop {
	margin: 0px;
	position: relative;
	width: 510px;
	text-align: right;
	background-color: #DADADA;
	padding: 5px;
}

div#checkBody {
	margin: 0px;
	position: relative;
	width: 510px;
	text-align: left;
	background-color: #EEEEEE;
	padding: 5px;
}

div#checkBottom {
	position: relative;
	width: 510px;
	text-align: left;
	font: monospace;
	background-color: #DADADA;
	padding: 5px;
}

td#checkAmount {
	clear: both;
	background-color: #FFFFFF;
	text-align: right;
}

div#checkDate {
	text-align: right;
	float: right;
	font-size: 1em;
}

div#checkAddr {
	text-align: left;
	float: left;
	font-size: 1em;
}

div#check {
	padding-top: 5em;
	text-align: left;
	float: none;
}

#normalRight {
	text-align: right;
}

#smallFont {
	font-size: .75em;
}

#smallFontBottom {
	font-size: .75em;
	vertical-align: bottom;
}

#underline {
	border-bottom-color: black;
	border-bottom-style: solid;
	border-bottom-width: 1px;
}
