.tahoma12bBlue {
	font: bold 12px Tahoma, Arial, sans-serif;
	color: #0033CC;
	text-decoration: none;
	margin: 0px;
	padding: 2px;
	text-indent: 2pt;
	vertical-align: middle;
}

.copyright {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 9px;
	color: #000000;
}

.borderless {
	margin: 0px;
	padding: 0px;
	border: 0px none;
}

.fieldShadow {
	background-image: url(../images-old/field_back.gif);
	background-repeat: no-repeat;
	background-position: -1px -1px;
	margin: auto;
	padding: 3px 0px 0px 3px;
	text-indent: 2px;
	clear: none;
	float: none;
	border: 1px solid #999999;
	line-height: 12px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #000000;
	height: 22px;
}

.recordList {
	font-family: Tahoma, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #000000;
	border-bottom-width: 1px;
	border-bottom-style: dotted;
	border-bottom-color: #CCCCCC;
}

.tabCursor {
	font-family: Tahoma, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #000000;
	cursor: hand;
}

.tahoma12b {
	font: bold 12px/12px Tahoma, Arial, sans-serif;
	color: #000000;
}

.tahoma12bBlueCursor {
	font: bold 12px Tahoma, Arial, sans-serif;
	color: #0000FF;
	text-decoration: none;
	cursor: hand;
}

.tahoma12 {
	font: normal 14px Tahoma, Arial, sans-serif;
	color: #000000;
	text-decoration: none;
}

.tahoma12bWhiteShadow {
	font: bold 12px/12px Tahoma, Arial, sans-serif;
	color: #FFFFFF;
	filter: Shadow(Color = #000000, Direction = 135);
}

.tahoma12bBlackGlow {
	font: bold normal 12px Tahoma, Arial, sans-serif;
	color: #000000;
	filter: DropShadow(Color = FFFFFF, OffX = 1, OffY = 1,);
}

.tahoma11Cursor {
	font: normal 11px/21px Tahoma, Arial, sans-serif;
	color: #000000;
	text-decoration: none;
	cursor: hand;
	height: 21px;
}

.menuSeperator {
	background: #FFFFFF url(../images-old/1Grey.gif) repeat-x;
	overflow: visible;
	line-height: 21px;
}

.menuover {
	font: normal 11px Tahoma, Arial, sans-serif;
	color: #FFFFFF;
	background: #0000FF;
}

.dropmenuitem {
	font: normal 11px/21px Tahoma, Arial, sans-serif;
	color: #000000;
	cursor: hand;
	height: 21px;
	width: 130px;
}

.dropmenuover {
	font: normal 11px Tahoma, Arial, sans-serif;
	color: #FFFFFF;
	background: #0000FF;
	height: 21px;
	width: 130px;
	cursor: hand;
}

.menuItems {
	font: normal 11px/21px Tahoma, Arial, sans-serif;
	color: #000000;
	text-decoration: none;
	cursor: hand;
}

.p7tbsub {
	background-color: #FFFFFF;
	border: 1px solid #CCCCCC;
	layer-background-color: #FFFFFF;
	font: 11px/21px Tahoma, Arial, sans-serif;
	color: #000000;
}

.p7tbsub p {
	margin: 0px;
	line-height: 21px;
	height: 21px;
	width: 150px;
	text-decoration: none;
	text-indent: 4px;
	padding: 0px;
}

.p7tbsub a:link {
	color: #000000;
	line-height: 21px;
	height: 21px;
	width: 150px;
	text-decoration: none;
	padding-right: 4px;
	padding-left: 4px;
}

.p7tbsub a:visited {
	color: #000000;
	line-height: 21px;
	height: 21px;
	width: 150px;
	text-decoration: none;
	padding-right: 4px;
	padding-left: 4px;
}

.p7tbsub a:hover {
	color: #FFFFFF;
	background: #0000FF;
	width: 150px;
	text-decoration: none;
	line-height: 21px;
	padding-right: 4px;
	padding-left: 4px;
	height: 21px;
}

.p7tbsub a:active {
	color: #FFFFFF;
	background: #0000FF;
	height: 21px;
	width: 150px;
	text-decoration: none;
	padding-right: 4px;
	padding-left: 4px;
	line-height: 21px;
}

.p7tbdn {
	color: #FFFFFF !important;
	font-weight: bold;
	background: #0000FF;
	text-decoration: none;
}

.P8tbsub {
	background-color: #FFFFFF;
	border: .1px solid #FFFFFF;
	font-size: 12px;
	font-family: Arial, Helvetica, sans-serif;
	layer-background-color: #FFFFFF;
}

.P8tbsub p {
	margin: 0px;
	padding: 6px 12px 12px 0px;
}

.P8tbsub a:link {
	color: #000000;
}

.P8tbsub a:visited {
	color: #666666;
}

.P8tbsub a:hover {
	color: #FF9900;
}

.P8tbsub a:active {
	color: #FF9900;
}

.P8tbdn {
	color: #FF9900 !important;
	font-weight: bold;
}

